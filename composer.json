{
  "name": "blueeon/simple-message-yii2",
  "type": "yii2-extension",
  "description": "A simple private message module for yii2, only contain model and API, no web interface.",
  "license": "MIT",
  "keywords": [
    "private message",
    "message",
    "yii2"
  ],
  "homepage": "https://github.com/blueeon/simple-message-yii2",
  "authors": [{
    "name": "blueeon",
    "email": "blueeon@blueeon.net",
    "role": "Developer"
  }],
  "require": {
    "yiisoft/yii2": "^2.0.0"
  },
  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3"
  },
  "autoload": {
    "psr-4": {
      "blueeon\\Message\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  }
}